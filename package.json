{
  "name": "training-app",
  "version": "0.0.4",
  "description": "Noora Health's patient and caregiver training app",
  "scripts": {
    "prod": "meteor run --settings settings.json",
    "prod-ios": "meteor run ios-device --settings settings.json",
    "dev": "meteor run --settings settings_dev.json",
    "dev-ios": "meteor run ios-device --settings settings_dev.json",
    "android": "meteor run android --settings settings_dev.json --verbose",
    "android-device": "meteor run android-device --settings settings_dev.json --verbose",
    "build-ios": "meteor run ios-device --mobile-server http://trainingapp.noorahealth.org --settings settings.json",
    "build": "meteor build ../training-app-build --server=http://52.53.187.70:80"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "cloudinary": "^1.4.4",
    "meteor-node-stubs": "^0.2.3"
  }
}
